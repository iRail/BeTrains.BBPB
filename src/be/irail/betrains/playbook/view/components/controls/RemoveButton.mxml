<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009"
		 xmlns:s="library://ns.adobe.com/flex/spark"
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 xmlns:ns="http://ns.renaun.com/mxml/2010"
		 xmlns:buttons="qnx.ui.buttons.*"
		 creationComplete="creationCompleteHandler()">
	<fx:Script>
		<![CDATA[
			import mx.events.FlexEvent;

			import qnx.ui.core.SizeMode;

			private function creationCompleteHandler():void {
				searchBtn.setSize(50, 50);
				searchBtn.setIcon("images/trash.png");
			}
		]]>
	</fx:Script>
	<fx:Declarations>
	</fx:Declarations>
	<fx:Metadata>
	[Event(name="delete",type="flash.events.Event")]
</fx:Metadata>
	<s:Rect left="0"
			right="0"
			top="0"
			bottom="0">
		<s:fill>
			<s:SolidColor color="0"
						  alpha=".4"/>
		</s:fill>
	</s:Rect>

	<s:Group width="50"
			 height="50"
			 verticalCenter="0"
			 right="5">
		<ns:QContainer width="50"
					   height="50">
			<buttons:IconButton id="searchBtn"
								click="dispatchEvent(new Event('delete'));"/>
		</ns:QContainer>
	</s:Group>


</s:Group>
